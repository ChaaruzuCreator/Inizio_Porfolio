{
  "version": 2,
  "builds": [
    {
      "src": "html/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "javascript/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "css/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "Video/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "Logo/**/*.{png,jpg,jpeg,gif,ico,svg}",
      "use": "@vercel/static"
    },
    {
      "src": "Photo/**/*.{png,jpg,jpeg,gif,ico,svg}",
      "use": "@vercel/static"
    },
    {
      "src": "Fonts/**/*.{ttf,otf,woff,woff2}",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/html/Portfolio.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/javascript/(.*)",
      "dest": "/javascript/$1"
    },
    {
      "src": "/Video/(.*)",
      "dest": "/Video/$1"
    },
    {
      "src": "/Logo/(.*)",
      "dest": "/Logo/$1"
    },
    {
      "src": "/Photo/(.*)",
      "dest": "/Photo/$1"
    },
    {
      "src": "/Fonts/(.*)",
      "dest": "/Fonts/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/html/$1"
    }
  ],
  "functions": {
    "html/*.html": {
      "includeFiles": "**"
    }
  }
}
