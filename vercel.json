{
  "version": 2,
  "builds": [
    {
      "src": "html/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "css/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "video/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/html/Portfolio.html"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/video/(.*)",
      "dest": "/video/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/html/$1"
    }
  ]
}
