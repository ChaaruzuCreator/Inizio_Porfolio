{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "dest": "/html/Portfolio.html"
    },
    {
      "src": "/index.html",
      "dest": "/html/Portfolio.html"
    },
    {
      "src": "/html/(.*)",
      "dest": "/html/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/javascript/(.*)",
      "dest": "/javascript/$1"
    },
    {
      "src": "/Logo/(.*)",
      "dest": "/Logo/$1"
    },
    {
      "src": "/Photo/(.*)",
      "dest": "/Photo/$1"
    },
    {
      "src": "/Video/(.*)",
      "dest": "/Video/$1"
    },
    {
      "src": "/Fonts/(.*)",
      "dest": "/Fonts/$1"
    }
  ]
}
